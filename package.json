{
  "name": "react-forms-store-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/react-forms-store",
    "packages/react-forms-store-ux",
    "packages/react-forms-dev"
  ],
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "storybook": "yarn workspace react-forms-dev storybook",
    "format": "yarn prettier --write packages | grep -v unchanged",
    "lint": "eslint"
  },
  "packageManager": "yarn@4.6.0",
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-plugin-jest": "^28.11.0",
    "inquirer": "^12.4.3",
    "merge-prettier-plugins": "^1.0.3",
    "prettier": "^3.5.1",
    "prettier-plugin-import-sorting": "^1.1.23",
    "prettier-remove-unused-imports": "^1.1.21",
    "semver": "^7.7.1"
  }
}
