  "dependencies": {
    "i18next": "^24.2.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "esbuild": "^0.14.0",
    "i18next-parser": "^9.3.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.0.0"
  },
  "packageManager": "yarn@4.5.1"
